<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="health_record_seq" model="ir.sequence">
        <field name="name">Health Record</field>
        <field name="code">health.record</field>
        <field name="prefix">HREC-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>
    <record id="prescription_record_seq" model="ir.sequence">
        <field name="name">Prescription Record</field>
        <field name="code">medical.prescription</field>
        <field name="prefix">PRSN-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>
    <record id="custom_journal_seq" model="ir.sequence">
        <field name="name">Journals</field>
        <field name="code">custom.journal</field>
        <field name="prefix">H-INV-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>